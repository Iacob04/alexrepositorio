		for (int i = 0; i < 4 ; i++) {
	            int num = (int)(Math.random() * 10) ;
	            System.out.print(num);
	           
		 } System.out.print("-");
		for (int i = 0; i < 4 ; i++) {
            int num = (int)(Math.random() * 10) ;
